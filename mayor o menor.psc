Algoritmo si_entonce_DavidAguirre
	definir dav Como Entero;
	definir agui como entero;
	Escribir "Escriba el primer numero para comparar";
	leer dav;
	Escribir "Escriba el segundo numero"; 
	leer agui;
	si dav > agui Entonces
		escribir dav "es mayor que" agui;
	SiNo
		escribir dav  "es menor igual que " agui;
	FinSi
	
FinAlgoritmo
